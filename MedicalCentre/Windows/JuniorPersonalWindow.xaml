<Window x:Class="MedicalCentre.Windows.JuniorPersonalWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MedicalCentre.Windows"
        mc:Ignorable="d"
        Title="JuniorPersonalWindow" Height="450" Width="800">
    <Grid>
        <Menu Height="17.5" VerticalAlignment="Top" HorizontalAlignment="Stretch" Background="LightGray">
            <MenuItem Header="Show" Height="17.5">
                <MenuItem Header="Storage" Command="{Binding ShowStorageItemsCommand}"/>
            </MenuItem>
        </Menu>
        <Frame NavigationUIVisibility="Hidden" Name="frame"/>
    </Grid>
</Window>
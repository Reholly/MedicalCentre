<Page x:Class="MedicalCentre.Pages.JuniorPersonalWindowPages.PatientRegistrationPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MedicalCentre.Pages.JuniorPersonalWindowPages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PatientRegistrationPage"
      Background="AliceBlue">

    <Grid>
        <Menu VerticalAlignment="Top" HorizontalAlignment="Stretch" Background="LightGray" Height="18">
            <MenuItem Header="CheckById" Command="{Binding CheckPatientByIdCommand}" Height="18"/>
        </Menu>
    </Grid>
</Page>
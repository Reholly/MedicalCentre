<Page x:Class="MedicalCentre.Pages.AdminWindowPages.AnalyticsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MedicalCentre.Pages.AdminWindowPages"
      xmlns:s="http://schemas.abtsoftware.co.uk/scichart"
      
      mc:Ignorable="d" 
      d:DesignHeight="1080" d:DesignWidth="1610"
      Title="Аналитика"
      ShowsNavigationUI="False">

    <Grid>
        <Border >
            <Border.Background>
                <LinearGradientBrush StartPoint="1,0" EndPoint="0,1">
                    <GradientStop Color="AliceBlue" Offset="0"/>
                    <GradientStop Color="#26527c" Offset="1.3"/>
                </LinearGradientBrush>
            </Border.Background>
            <Grid>
                <WpfPlot Name="MainChart" VerticalAlignment="Top"/>
            </Grid>
        </Border>
    </Grid>
</Page>

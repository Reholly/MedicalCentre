<Page x:Class="MedicalCentre.Pages.AdminWindowPages.StoragePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MedicalCentre.Pages.AdminWindowPages"
      mc:Ignorable="d" 
            d:DesignHeight="1080" d:DesignWidth="1610"
      Title="Аналитика"
      ShowsNavigationUI="False">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>

        <Border Grid.ColumnSpan="2">
            <Border.Background>
                <LinearGradientBrush StartPoint="1,0" EndPoint="0,1">
                    <GradientStop Color="AliceBlue" Offset="0"/>
                    <GradientStop Color="#26527c" Offset="1.3"/>
                </LinearGradientBrush>
            </Border.Background>
            <TextBlock HorizontalAlignment="Center" Foreground="Black" FontSize="55" VerticalAlignment="Center">
                <TextBlock.Text>типа складской учет</TextBlock.Text>
            </TextBlock>

        </Border>

    </Grid>
</Page>

<Page x:Class="MedicalCentre.Pages.DoctorWindowPages.PatientExaminationsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MedicalCentre.Pages.DoctorWindowPages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Patient's Examinations "
      Background="Transparent"
      ShowsNavigationUI="False">
    <Grid Margin="20">
        <DataGrid Background="AliceBlue" ItemsSource="{Binding Examinations}" 
                  Grid.Column="0" AutoGenerateColumns="True" CanUserAddRows="False" CanUserDeleteRows="False"/>
    </Grid>
</Page>